{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:react/recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": "off",
        "react/jsx-key": [1, { "checkFragmentShorthand": true }],
        "react/react-in-jsx-scope": "off",
        "react/jsx-uses-react": "off"
    }
}
